<div class="row row-margin">
  <div class="col-md-2"></div>
  <div class="col-md-8">

  <%= link_to "Add Student Account(s)", new_student_path, class: "btn btn-primary" %>
  <br>
  <br>
  <table class="table table-striped table-bordered student">
    <thead>
      <tr>
        <th>Email Address</th>
        <th>Created At</th>
      </tr>
    </thead>
    <tbody>
      <% @students.each do |student| %>
        <tr>
          <td><%= student.email %></td>
          <td><%= student.created_at.strftime("%A, %d %b %Y %l:%M %p") %></td>
        </tr>
      <% end %>
    </tbody>
  </table>



  </div>
<div class="col-md-2"></div>
</div>