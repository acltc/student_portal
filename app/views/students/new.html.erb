<div class="row row-margin">
  <div class="col-md-2"></div>
  <div class="col-md-8">
  <%= form_tag students_path do %>
    <div class="form-group">
      <%= label_tag :student_emails, "Enter all new student email addresses." %><br />
      <%= text_area_tag :student_emails, nil, class: "form-control" %>
      <p class="help-block">Seperate by a comma.</p>
    </div>
    <div class="form-group">
      <%= submit_tag "Create Accounts", class: "btn btn-primary btn-lg" %>
    </div>
  <% end %>
  </div>
<div class="col-md-2"></div>
</div>